<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
    <id>android-dependencies</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/</outputDirectory>
            <useProjectArtifact>true</useProjectArtifact>
            <unpack>true</unpack>
            <excludes>
                <exclude>org.json:json</exclude>
                <exclude>log4j:log4j</exclude>
                <exclude>org.slf4j:slf4j-api</exclude>
            </excludes>
        </dependencySet>
    </dependencySets>
    <files>
        <file>
            <source>target/classes/org/jboss/netty/channel/AbstractChannel.class</source>
            <outputDirectory>/org/jboss/netty/channel</outputDirectory>
            <filtered>false</filtered>
        </file>
        <file>
            <source>target/classes/org/jboss/netty/channel/AbstractChannel$ChannelCloseFuture.class</source>
            <outputDirectory>/org/jboss/netty/channel</outputDirectory>
            <filtered>false</filtered>
        </file>
        <file>
            <source>target/classes/org/jboss/netty/channel/socket/oio/OioWorker.class</source>
            <outputDirectory>/org/jboss/netty/channel/socket/oio</outputDirectory>
            <filtered>false</filtered>
        </file>
    </files>
</assembly>